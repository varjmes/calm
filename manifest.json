{
  "name": "Calm",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Desaturate a page by 50%",
  "homepage_url": "https://twitter.com/varjmes",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Calm"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
